package  {		import flash.display.Sprite;	import flash.system.Capabilities;	import flash.display.StageQuality;	public class Iconic extends Sprite	{		public function Iconic() 		{			super();			init();		}				private function init():void		{			stage.quality = StageQuality.BEST;			stage.nativeWindow.x = Math.round( ( Capabilities.screenResolutionX - width ) / 2 );			stage.nativeWindow.y = Math.round( ( Capabilities.screenResolutionY - height ) / 2 );			stage.nativeWindow.visible = true;		}	}}